<?xml version="1.0" encoding="utf-8"?>
<Component
        xmlns="http://www.red5studios.com/"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.red5studios.com/ ../../../schemas/component.xsd"
    >

    <Info
        title="bAutoRepair"
        author="BurstBiscuit"
        description="Automatically repairs your equipment when visiting a battleframe garage"
        url="https://bitbucket.org/0x4D61696B/bautomaticrepairs"
        uiVersion="1000"
    />

    <Events>
        <Event name="ON_REPAIR_RESPONSE"            bind="OnRepairResponse" />
        <Event name="ON_TERMINAL_AUTHORIZED"        bind="OnTerminalAuthorized" />
        <Event name="MY_ACTIVITY_TRACKER_UPDATE"    bind="OnTrackerUpdate" />
    </Events>
</Component>
